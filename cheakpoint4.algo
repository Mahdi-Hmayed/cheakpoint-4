ALGORITHM Tri_Insertion
VAR
     i, j, n : INTEGER
     tab : array_name : ARRAY_OF INTEGER[n]
     tab : array_name : ARRAY_OF INTEGER[n]
BEGIN
    FOR i FROM 0 TO n STEP 1 DO
        v = tab[i] ;
            j=i ;
            WHILE (tab[j-1]>v) DO
                tab[j]=tab[j-1] ;
                j=j-1
            END_WHILE
            tab[j]=v 
    END_FOR  
END
  